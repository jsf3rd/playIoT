function THanConvert._DoAutoCheck(const Buf: AnsiString): THCStyle;
const
  WIndex: Array [1 .. 10] of AnsiString = (' ', ' ', ' ', ' ', ' ', ' ', ' ', '',
    '.', '.');
  JIndex: Array [1 .. 10] of AnsiString = ('e ', 'e ', ' ', 'a ', 'i ', 'i ', ' ', '',
    'a.', 'a.');
var
  J, W, I: Integer;
begin
  J := 0;
  W := 0;
  For I := 1 to 10 do
  begin
    if Pos(WIndex[I], Buf) > 0 then
      Inc(W);
    if Pos(JIndex[I], Buf) > 0 then
      Inc(J);
  end;
  (* *) if J > W then
    Result := hcJohab
  else if W > J then
    Result := hcWansung
  else
    Result := hcUnknown;

  if Assigned(FOnAutoCheck) then
    FOnAutoCheck(Self, Buf, Result);
end;

function THanConvert._DoConvert(const Flag: THCConvertFlags; var Buf: AnsiString): Boolean;
begin
  Result := True;
  case Flag of
    hcAuto:
      case _DoAutoCheck(Buf) of
        hcUnknown:
          Result := False;
        hcJohab:
          _Conv_J2W(Buf);
        hcWansung:
          _Conv_W2J(Buf);
      end;
    hcJ2W:
      _Conv_J2W(Buf);
    hcW2J:
      _Conv_W2J(Buf);
  end;
end;

function THanConvert.DoAutoCheck: THCStyle;
begin
  Result := _DoAutoCheck(AnsiString(FLines.Text));
end;

function THanConvert.DoConvert: Boolean;
var
  Buf: AnsiString;
begin
  Buf := AnsiString(FLines.Text);
  Result := _DoConvert(Flags, Buf);
  FLines.Text := String(Buf);
end;

function THanConvert.ConvertFile(const Flag: THCConvertFlags;
  const SRC, DST: TFileName): Boolean;
var
  sBuf: TStrings;
  Buf: AnsiString;
begin
  sBuf := TStringList.Create;
  try
    begin
      sBuf.LoadFromFile(SRC);
      Buf := AnsiString(sBuf.Text);
      Result := _DoConvert(Flag, Buf);
      if Result then
      begin
        sBuf.Text := String(Buf);
        Try
          sBuf.SaveToFile(DST)
        except
          Result := False
        end;
      end;
    end
  finally
    sBuf.Free;
  end
end;

