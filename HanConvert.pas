        PrePerc := I;
      end;
    if FProcessMessage then
      Application.ProcessMessages;
    if (Byte(Buf[I]) and $80) = 0 then
    begin
      Inc(I);
      Continue;
    end;
    b1 := Byte(Buf[I]);
    b2 := Byte(Buf[I + 1]);
    KS2KSC_C(b1, b2);
    Result[I - 1] := b1;
    Result[I] := b2;
    Inc(I, 2);
  end;

  if Assigned(FOnProcessStatus) then
    FOnProcessStatus(Self, Len, Len);
end;

