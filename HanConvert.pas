function THanConvert.KS2KSC_C(var C1, C2: Byte): Word;
var
  I: Integer;
  Handled: Boolean;
begin
  I := (C1 - $A1) * $5E + C2 - $A1;
  if I > MaxTable then
  begin
    if Assigned(FOnCannotConvert) then
    begin
      Handled := False;
      FOnCannotConvert(Self, C1, C2, Handled);
      if Handled then
      begin
        Result := (Word(C1) shl 8) or Word(C2);
        Exit;
      end;
    end;
    C1 := $20;
    C2 := $20;
    Result := $2020;
  end
  else
  begin
    Result := KSCODE_TABLE[I];
    C2 := Byte(Result and $FF);
    C1 := Byte(Result shr 8);
  end;
end;

function THanConvert._binsearch(min, max: Integer; const code: Word): Word;
label ReDO;
var
  center: Integer;
begin
ReDO:
  if min >= max then
  begin
    if (KSCODE_TABLE[min] = code) then
      Result := min
    else
      Result := $FFFF;
    Exit;
  end;
  center := (min + max) shr 1;

  // while (KSCODE_TABLE[center]=$2020) do Dec(center);

  if (KSCODE_TABLE[center] = code) then
    Result := center
  else if (KSCODE_TABLE[center] > code) then
    Result := _binsearch(min, center - 1, code)
  else
    Result := _binsearch(center + 1, max, code);
end;

function THanConvert.KSC2KS_C(var C1, C2: Byte): Word;
var
  I: Word;
  Handled: Boolean;
begin
  I := Word(C2) or (Word(C1) shl 8);

  case I of
    $D931 .. $DEFE:
      I := _binsearch(0, 1127, I);
    $D480 .. $D4FE:
      I := _binsearch(1128, 1255, I);
    $8861 .. $D3B7:
      I := _binsearch(1410, 3759, I);
    $E031 .. $F9FE:
      I := _binsearch(3854, 8741, I);
  else
    I := $FFFF;
  end;

  if I = $FFFF then
  begin
    if Assigned(FOnCannotConvert) then
    begin
      Handled := False;
      FOnCannotConvert(Self, C1, C2, Handled);
      if Handled then
      begin
        Result := (Word(C1) shl 8) or Word(C2);
        Exit;
      end;
    end;
    C1 := $20;
    C2 := $20;
    Result := $2020;
  end
  else
  begin
    C1 := (I div $5E) + $A1;
    C2 := (I mod $5E) + $A1;
    Result := (Word(C1) shl 8) or Word(C2);
  end;
end;

procedure THanConvert._Conv_J2W(var Buf: AnsiString);
var
  I, Len: Integer;
  PrePerc, DotPerc: Integer;
  b1, b2: Byte;
begin
  Len := Length(Buf);
  if Len < 2 then
    Exit;
  DotPerc := (Len div 200) + 1;
  PrePerc := 0;
  if Assigned(FOnProcessStatus) then
    FOnProcessStatus(Self, 0, Len);

  I := 1;
  while I < Len do
  begin
    if Assigned(FOnProcessStatus) then
      if I - PrePerc > DotPerc then
      begin
        FOnProcessStatus(Self, I, Len);
        PrePerc := I;
      end;
    if FProcessMessage then
      Application.ProcessMessages;
    if (Byte(Buf[I]) and $80) = 0 then
    begin
      Inc(I);
      Continue;
    end;
    b1 := Byte(Buf[I]);
    b2 := Byte(Buf[I + 1]);
    KSC2KS_C(b1, b2);
    Buf[I] := AnsiChar(b1);
    Buf[I + 1] := AnsiChar(b2);
    Inc(I, 2);
  end;

  if Assigned(FOnProcessStatus) then
    FOnProcessStatus(Self, Len, Len);
end;

